{
  "name": "fast-flux-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:aws": "$env:NODE_ENV='production'; next build && aws s3 sync ./out s3://$env:AWS_BUCKET_NAME",
    "deploy:aws": "npm run build:aws && aws cloudfront create-invalidation --distribution-id $env:AWS_CLOUDFRONT_ID --paths '/*'",
    "build:worker": "$env:NODE_ENV='production'; cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && wrangler deploy"
  },
  "dependencies": {
    "critters": "^0.0.23",
    "next": "14.2.13",
    "react": "^18",
    "react-dom": "^18",
    "replicate": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.0.3",
    "@types/node": "20.17.17",
    "@types/react": "18.3.18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "5.7.3",
    "wrangler": "^3.80.2"
  }
}
